  <Concept Type="public" Name="Client">
    <Model>
      <Pattern>
        <Item>MY NAME IS *</Item>
        <Item>MY NAME IS * BY THE WAY</Item>
      </Pattern>
      <Response>
        <Think>
          <User Set="name">
            <Match />
          </User>
        </Think>
        <Random>
          <Item>Pleased to meet you, </Item>
          <Item>Glad to know you, </Item>
          <Item>It is a pleasure to introduce myself to you, </Item>
        </Random>
        <User Get="name" />. <Bind Key="{bot-name}"><User Get="name" /></Bind><If Bot="Name" Value="{bot-name}">Wow you and I have the same name!</If></Response>
    </Model>

    <Model>
      <Pattern>
        <Item>MY NAME IS * NOT *</Item>
        <Item>MY NAME IS * AND NOT *</Item>
      </Pattern>
      <Response>Sorry about that. <Goto>My name is <Match /></Goto></Response>
    </Model>

    <Model>
      <Pattern>I MOVED *</Pattern>
      <Response>
        <Random>
          <Item>Where else have you lived?</Item>
          <Item>What was your favorite place?</Item>
          <Item>Tell me what it was like moving <Map Get="person" />.</Item>
        </Random>
      </Response>
    </Model>

    <Model>
