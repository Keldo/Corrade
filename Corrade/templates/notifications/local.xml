<?xml version="1.0" encoding="utf-16"?>
<SerializedNotification xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Notification>local</Notification>
  <NotificationParameters>
    <Item>
      <Key>
        <string>OpenMetaverse.ChatEventArgs</string>
      </Key>
      <Value>
        <ArrayOfParameter>
          <Parameter>
            <Path>FromName</Path>
            <Name>name</Name>
            <Processing>
              <NameSplit>
                <First>firstname</First>
                <Last>lastname</Last>
                <Condition>
                  <Path>SourceType</Path>
                  <ChatSourceType>Agent</ChatSourceType>
                </Condition>
              </NameSplit>
            </Processing>
          </Parameter>
          <Parameter>
            <Path>Message</Path>
            <Name>message</Name>
            <Processing>
              <IdentifyLanguage>
                <Name>language</Name>
              </IdentifyLanguage>
            </Processing>
          </Parameter>
          <Parameter>
            <Path>OwnerID</Path>
            <Name>owner</Name>
          </Parameter>
          <Parameter>
            <Path>SourceID</Path>
            <Name>item</Name>
          </Parameter>
          <Parameter>
            <Path>Position</Path>
            <Name>position</Name>
          </Parameter>
          <Parameter>
            <Path>SourceType</Path>
            <Name>entity</Name>
          </Parameter>
          <Parameter>
            <Path>AudibleLevel</Path>
            <Name>audible</Name>
          </Parameter>
          <Parameter>
            <Path>Type</Path>
            <Name>volume</Name>
          </Parameter>
        </ArrayOfParameter>
      </Value>
    </Item>
  </NotificationParameters>
</SerializedNotification>